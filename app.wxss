/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
} 
button{
  font-size: 32rpx;
  background: #005faf;
  color: #fff;
  border-radius: 0;
  transition: all .3s;
  letter-spacing: 1rpx;
}
button:active{
  background: rgba(0,95,175,.7);
  color: #fff;
}
button::after{
  border: 0 none;
}
text{font-size: 28rpx;}
.no_data{
  position:fixed;
  top:0;
  bottom:0;
  width:100%;
  display:flex;
  align-items:center;
  justify-content:center;
}
.no_data icon{
  font-size:200rpx;
  color: #999999;
}
.wux-rater {
  display: flex;
  align-items: center;
  margin: 0;
  padding: 0;
  line-height: normal;
  vertical-align: middle;
  font-weight: 400;
  font-style: normal;
}
.wux-rater:before,
.wux-rater:after {
  display: table;
  content: " ";
}
.wux-rater:after {
  clear: both;
}
.wux-rater:before,
.wux-rater:after {
  display: table;
  content: " ";
}
.wux-rater:after {
  clear: both;
}
.wux-rater__box {
  position: relative;
  display: block;
  float: left;
  text-align: center;
  cursor: pointer;
  color: #ccc;
  -webkit-transition: color .1s ease;
  transition: color .1s ease;
}
.wux-rater__box:last-child {
  padding-right: 4rpx !important;
  margin-right: 0 !important;
}
.wux-rater__box--disabled {
  color: #ccc !important;
  cursor: not-allowed;
}
.wux-rater__inner {
  position: relative;
  display: block;
}
.wux-rater__outer {
  position: absolute;
  left: 0;
  top: 0;
  display: block;
  overflow: hidden;
}
.wux-rater__text {
  display: inline-block;
  float: none;
  margin-left: 20rpx;
  vertical-align: middle;
  font-size: 32rpx;
}
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family:"iconfont" !important;
  font-size:16px;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

